#ifdef  /   #idndef
#endif

#include <iostream>
#define NUM 3

int main(){
#ifdef DEBUG
    printf("DEBUG宸插畾涔塡n");
#endif
    for (int i = 0; i < NUM; i++){
        std::cout << "Hello G++!" << std::endl;
    }
    return 0;
}

cmake_minimum_required(VERSION 3.15)
project(test)
add_definitions(-DDEBUG)
add_executable(app ../source/test.cpp)
