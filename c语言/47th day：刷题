int main(){
    int x,  N, judge = 0;
    scanf("%d",&N);
    
    for ( x = 1; x*x <= N/2; x++){
        int y_squared = N - x*x;
        int y = (int)sqrt(y_squared);
        
        if (y*y + x*x== N){
            printf("%d %d\n",x,y);
            judge = 1;
        }
    }

    if (!judge){
        puts("No Solution");
    }
    
    return 0;
}
