int main()
{
	float a = 0;
	float b = 0;
	scanf("%f %f",&a, &b);
	float sum = a + b;
	printf("sum=%f+%f=%f\n", a, b, sum);
	printf("sum++=%f\n", sum++);
	printf("sum2=%f\n",sum);
	printf("++sum=%f\n", ++sum);
	printf("sum3=++sum=%f\n",sum);
	float sum4 = (int)(sum);
	printf("sum4=%f\n", sum4);
	printf("sum4/=3\n    â†“\nsum5=sum4/3=%f\n", sum4/=3);
	return 0;
}
