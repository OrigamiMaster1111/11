bool isShuixian(int digit){
    int sum = 0, temp = digit;
    while (temp != 0){
        sum += pow((temp%10), 3);
        temp /= 10;
    }

    if (sum == digit){
        return true;
    } else {
        return false;
    }
}

int main(){
    int digit = 100;
    for ( ; digit < 1000; digit++){
        if (isShuixian(digit)){
            printf("%d ", digit);
        }
    }
    return 0;
}
