int main(){
    int year;
    int time;
    scanf("%d %d",&year,&time);
    if(year < 5){
        double salary = time > 40 ? 40*30 + (time - 40)*1.5*30 : time*30;
        printf("%.2lf",salary);
    }    else{
        double salary = time > 40 ? 40*50 + (time - 40)*1.5*50 : time*50;
        printf("%.2lf",salary);
    }
    return 0;
}
