int main()
{
	double foot = 0;
	double inch = 0;
	printf("请输入身高的英尺和英寸");
	scanf("%lf %lf",&foot,&inch);
	printf("身高为%f米\n",(foot + inch / 12) * 0.3048);
	return 0;
}

