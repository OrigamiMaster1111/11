int main()
{
	int Receivable = 0;
	int Paid = 0;
    printf("欢迎使用自助收银系统\n应收款(元):");
	scanf("%d", &Receivable);
	printf("实付(元):");
	scanf("%d", &Paid);
	int Change = Paid - Receivable;
	printf("找零= %d - %d =%d\n", Paid, Receivable, Change);
	return 0;
}
